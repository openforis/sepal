{
    "name": "noiselabs/nusoap-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for NuSOAP - SOAP Toolkit for PHP",
    "keywords": ["soap", "symfony", "noiselabs"],
    "homepage": "https://github.com/noiselabs/NoiselabsNuSOAPBundle",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Vítor Brandão",
            "email": "vitor@noiselabs.org",
            "homepage": "http://noiselabs.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nusphere/nusoap",
                "version": "0.9.5",
                "dist": {
                    "url": "http://downloads.sourceforge.net/project/nusoap/nusoap/0.9.5/nusoap-0.9.5.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["lib/"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.0",
        "nusphere/nusoap": "0.9.5"
    },
    "autoload": {
        "psr-0": { "NoiseLabs\\Bundle\\NuSOAPBundle": "" }
    },
    "target-dir": "NoiseLabs/Bundle/NuSOAPBundle",
    "minimum-stability": "dev"
}
